	<!--根据主键查询记录-->
	<select id="selectById" resultType="TradeOrderItem">
		SELECT 
		<include refid="tradeOrderItemColumns"/>
		FROM trade_order_item a
		<include refid="tradeOrderItemJoins"/>
		WHERE a.order_item_id = #{id}
	</select>

	<!-- 根据多个id查询出一个列表 -->
	<select id="selectByIdIn" resultType="TradeOrderItem">
		SELECT 
		<include refid="tradeOrderItemColumns"/>
		FROM trade_order_item a
		<where>
			<choose>
				<when test="list != null and list.size >0">
					a.order_item_id in
					<foreach close=")" collection="list" item="item" open="(" separator=",">
						#{item}
					</foreach>
				</when>
				<otherwise> 1=2 </otherwise>
			</choose>
		</where>
	</select>

	<!--根据条件查询记录集-->
	<select id="selectByWhere" resultType="TradeOrderItem">
		SELECT <if test="w!=null and w.distinct">DISTINCT</if>
		<include refid="tradeOrderItemColumns"/>
		FROM trade_order_item a
		<include refid="tradeOrderItemJoins"/>
		<where>
			<if test="w!=null">
			<include refid="Where_Clause" />
			${w.outputSqlWhere}
			</if>
		</where>
		<choose>
			<when test="w!=null and w.outputSqlOrderBy !=null">
				${w.outputSqlOrderBy}
			</when>
			<when test="p!=null and p.orderBy != null">
				ORDER BY ${p.orderBy}
			</when>
			<otherwise>
				ORDER BY a.order_item_id DESC
			</otherwise>
		</choose>
	</select>

	<!--查询所有记录集，无条件-->
	<select id="selectAll" resultType="TradeOrderItem">
