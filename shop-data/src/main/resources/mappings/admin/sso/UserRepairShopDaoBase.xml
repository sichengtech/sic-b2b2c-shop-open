	<!--根据主键查询记录-->
	<select id="selectById" resultType="UserRepairShop">
		SELECT 
		<include refid="userRepairShopColumns"/>
		FROM user_repair_shop a
		<include refid="userRepairShopJoins"/>
		WHERE a.u_id = #{id}
	</select>

	<!-- 根据多个id查询出一个列表 -->
	<select id="selectByIdIn" resultType="UserRepairShop">
		SELECT 
		<include refid="userRepairShopColumns"/>
		FROM user_repair_shop a
		<where>
			<choose>
				<when test="list != null and list.size >0">
					a.u_id in
					<foreach close=")" collection="list" item="item" open="(" separator=",">
						#{item}
					</foreach>
				</when>
				<otherwise> 1=2 </otherwise>
			</choose>
		</where>
	</select>

	<!--根据条件查询记录集-->
	<select id="selectByWhere" resultType="UserRepairShop">
		SELECT <if test="w!=null and w.distinct">DISTINCT</if>
		<include refid="userRepairShopColumns"/>
		FROM user_repair_shop a
		<include refid="userRepairShopJoins"/>
		<where>
			<if test="w!=null">
			<include refid="Where_Clause" />
			${w.outputSqlWhere}
			</if>
		</where>
		<choose>
			<when test="w!=null and w.outputSqlOrderBy !=null">
				${w.outputSqlOrderBy}
			</when>
			<when test="p!=null and p.orderBy != null">
				ORDER BY ${p.orderBy}
			</when>
			<otherwise>
				ORDER BY a.u_id DESC
			</otherwise>
		</choose>
	</select>

	<!--查询所有记录集，无条件-->
	<select id="selectAll" resultType="UserRepairShop">
		SELECT <if test="w!=null and w.distinct">DISTINCT</if>
		<include refid="userRepairShopColumns"/>
		FROM user_repair_shop a
		<include refid="userRepairShopJoins"/>
		<choose>
			<when test="w!=null and w.outputSqlOrderBy !=null">
				${w.outputSqlOrderBy}
			</when>
			<otherwise>
				ORDER BY a.u_id DESC
			</otherwise>
		</choose>
	</select>
	
